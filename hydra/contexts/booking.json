{
    "@context": {
        "hydra": "http://www.w3.org/ns/hydra/core#",
        "vocab": "https://sws-group-7-hotel-api.herokuapp.com/api/v1/vocab#",
        "Booking": "vocab:Booking",
        "date": "vocab:Booking/date",
        "arrival_date": "vocab:Booking/arrivalDate",
        "depature_date": "vocab:Booking/depatureDate",
        "payment_method": "vocab:Booking/paymentMethod",
        "amount": "vocab:Booking/amount",
        "user": {
            "@id": "vocab:Booking/user",
            "@type": "@id"
        },
        "rooms": {
            "@id": "vocab:Booking/rooms",
            "@type": "@id"
        }
    }
}